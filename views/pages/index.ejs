<html lang="en">
  <head>
    <link rel="stylesheet" type="text/css" href="./styles/index.css" />
  </head>

  <body>
    <main>
      <div class="buttons">
        <a href="/" class="menu_b">
          <img src="../icons/rising.svg" alt="rising" />Rising
        </a>
        <a href="/categories" class="menu_b">
          <img src="../icons/categories.svg" alt="rising" />Categories
        </a>
      </div>

      <div class="grid-container">
        <% for (var i = 0; i < imageData.length; i++) { %>
        <div class="grid-item">
          <img
            src="<%= imageData[i].imageSrc %>"
            alt="<%= imageData[i].altText %>"
            class="image"
          />
        </div>
        <% } %>
      </div>
    </main>

    <script src="./scripts/index.js"></script>
  </body>
</html>
